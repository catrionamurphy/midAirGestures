 if numberHands == 2:
                handNo = 2
                lHand = frame.hands[1]
                rHand = frame.hands[0]
            
                if lHand.grab_strength == 1 and rHand.grab_strength == 1:
                    self.strengthList.append(lHand.grab_strength)
                elif lHand.grab_strength == 1 and rHand.grab_strength != 1:
                    continue
                elif lHand.grab_strength != 1 and rHand.grab_strength == 1:
                    continue
                else:
                    self.strengthList.append(lHand.grab_strength)
            elif numberHands == 1:
                handNo = 1
                if hand.grab_strength == 1:
                    self.strengthList.append(hand.grab_strength)
                else:
                    self.strengthList.append(hand.grab_strength)
        
            if len(self.strengthList) > 200 or numberHands == 0:
                for i in range(len(self.strengthList)-1):
                    if self.strengthList[i] == 1 and self.strengthList[i+1] < 1:
                        if handNo == 2:
                            print "DOUBLE FLASH"
                        elif handNo == 1:
                            print "FLASH"
                        else:
                            print "WAITING"
                    else:
                        continue
                self.strengthList = []